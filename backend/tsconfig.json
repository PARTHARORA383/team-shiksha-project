{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
      "noEmit": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "declaration": true,
    "sourceMap": true
  }
}
